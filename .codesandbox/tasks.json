{
  // These tasks will run in order when initializing your CodeSandbox project.
  "setupTasks": [
    {
      "name": "Install Dependencies",
      "command": "npm install"
    }
  ],

  // These tasks can be run from CodeSandbox. Running one will open a log in the app.
  "tasks": {
    "ng": {
      "name": "ng",
      "command": "npm run ng"
    },
    "start": {
      "name": "start",
      "command": "npm run start",
      "runAtStart": true
    },
    "build": {
      "name": "build",
      "command": "npm run build"
    },
    "watch": {
      "name": "watch",
      "command": "npm run watch"
    },
    "test": {
      "name": "test",
      "command": "npm run test"
    },
    "firebase-functions@0.7.0": {
      "name": "npm install --save-exact firebase-functions@0.7.0 npm install --save-exact firebase-admin@5.4.0",
      "command": "firebase-functions@0.7.0"
    },
    "firebase-functions": {
      "name": "firebase-functions",
      "command": "firebase-functions"
    },
    "npm install firebase-admin": {
      "name": "firebase-admin",
      "command": "npm install firebase-admin"
    },
    "npm install crypto-browserify": {
      "name": "npm install crypto-browserify stream-browserify assert stream-http https-browserify os-browserify",
      "command": "npm install crypto-browserify"
    },
    "npm install  assert stream-http": {
      "name": "npm install stream-browserify",
      "command": "npm install  assert stream-http"
    },
    "npm install  https-browserify": {
      "name": " https-browserify",
      "command": "npm install  https-browserify"
    },
    "npm install os-browserify": {
      "name": "os-browserify",
      "command": "npm install os-browserify"
    },
    "npm install videogular2 --save": {
      "name": "videogular2 --save",
      "command": "npm install videogular2 --save"
    },
    "videogular2 --save": {
      "name": "npm install",
      "command": "videogular2 --save"
    },
    "ng add @angular/material": {
      "name": "@angular/material",
      "command": "ng add @angular/material"
    },
    "npm install @angular/material": {
      "name": "@angular/material",
      "command": "npm install @angular/material"
    }
  }
}
